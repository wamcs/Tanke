<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/RelativeLayout"
        android:layout_width="match_parent"
        android:background="@color/colorPrimary"
        android:layout_height="@dimen/default_toolbar_height">

        <ImageView
            android:id="@id/protocol_last_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:padding="@dimen/x26"
            android:src="@mipmap/img_last_white"/>

        <com.lptiyu.tanke.widget.CustomTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/user_protocol"
            android:textColor="@color/default_font_color"
            android:textSize="@dimen/protocol_title_text_size"
            />
    </RelativeLayout>


    <WebView
        android:id="@id/protocol_web_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/RelativeLayout">

    </WebView>

</RelativeLayout>