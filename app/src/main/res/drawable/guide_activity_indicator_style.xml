<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <size android:height="@dimen/y18"
                android:width="@dimen/x18"/>
            <solid android:color="@color/guide_activity_indicator_solid_color"/>
            <stroke android:color="@color/guide_activity_indicator_stroke_color"
                android:width="@dimen/x1"/>
        </shape>
    </item>

    <item android:state_enabled="true">
        <shape android:shape="oval">
            <size android:height="@dimen/y18"
                  android:width="@dimen/x18"/>
            <stroke android:color="@color/guide_activity_indicator_stroke_color"
                android:width="@dimen/x1"/>
        </shape>
    </item>
</selector>